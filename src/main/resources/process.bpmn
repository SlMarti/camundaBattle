    <bpmn:sequenceFlow id="Flow_0co7dfu" name="true" sourceRef="Gateway_0qnlcad" targetRef="Activity_0zwn23p">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${army.size() &gt; 0 &amp;&amp; enemyWarriors &gt; 0}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="Activity_0zwn23p" name="Make decision?">
