    <bpmn:sequenceFlow id="Flow_0jrw1qa" sourceRef="Gateway_0dhhxc5" targetRef="Activity_1kw2nrw" />
    <bpmn:sequenceFlow id="Flow_0pwl15i" name="go!" sourceRef="Gateway_0dhhxc5" targetRef="Activity_08oorip">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${go == "go"}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
